    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@ngrx/signals": "^19.2.1",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@primeng/themes": "^19.1.3",
    "@tailwindcss/postcss": "^4.1.8",
    "postcss": "^8.5.4",
    "primeicons": "^7.0.0",
